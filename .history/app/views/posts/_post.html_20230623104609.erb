<div id="<%= dom_id post %>">
      <h2> 
      <strong>Title:</strong>
      <%= post.title %>
  




    <p>posted by
  <%= link_to post.user.name, user_path(post.user)%>
  </h2>
    <h6><%=pluralize(post.views, "view")%></h6>
  </p>

  <p>
    <strong>Body:</strong>
    <%= post.body %>
  </p>


</div>
